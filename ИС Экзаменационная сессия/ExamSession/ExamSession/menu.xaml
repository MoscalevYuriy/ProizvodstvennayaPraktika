        </StackPanel>
        
        <Frame x:Name="MyFrame" Grid.Column="1" HorizontalAlignment="Left" Height="472" NavigationUIVisibility="Hidden" VerticalAlignment="Top" Width="920" Background="#FFE0E0E0"/>
        

    </Grid>
    
</Window>
